{
  "expo": {
    "name": "Gymlink",
    "slug": "gymlink",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/f213c336-9501-40ed-a752-3339db5b1545"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "48",
      "bundleIdentifier": "com.devdylan.gymlink-app",
      "infoPlist": ""
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.devdylan.gymlink"
    },
    "extra": {
      "eas": {
        "projectId": "f213c336-9501-40ed-a752-3339db5b1545"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location. This will give you a tailored experience and allow $(PRODUCT_NAME) to display nearby gyms and potential like-minded gym goers on your feed."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "$(PRODUCT_NAME) will display your selected photos to other users on their feed. This will give you a tailored experience and allow $(PRODUCT_NAME) to display nearby gyms and potential like-minded gym goers on your feed."
        }
      ]
    ]
  }
}
